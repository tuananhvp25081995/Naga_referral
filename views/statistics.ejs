    <script src="/js/theme/bootstrap.min.js"></script>
    <!-- apps -->
    <script src="/js/theme/app.js"></script>
    <script src="/js/theme/app.init.dark.js"></script>
    <script src="/js/theme/app-style-switcher.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
