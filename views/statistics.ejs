    <script src="/js/theme/jquery.sparkline.js"></script>
    <!--Wave Effects -->
    <!-- <script src="/js/th    eme/waves.js"></script> -->
    <!--Menu sidebar -->
    <script src="/js/theme/sidebarmenu.js"></script>
    <!--Custom JavaScript -->
    <script src="/js/theme/feather.min.js"></script>
    <script src="/js/theme/custom.js"></script>
    <script src="/js/theme/jquery.dataTables.min.js"></script>
    <script src="/js/theme/custom-datatable.js"></script>


    <script src="https://cdn.datatables.net/buttons/1.5.1/js/dataTables.buttons.min.js"></script>
