                    </ul>
                </div>
            </nav>
        </header>

        <%- include('left_nav.ejs') %>

        <div class="page-wrapper">
            <div class="page-breadcrumb">
                <div class="row">
                    <div class="col-md-5 align-self-center">
                        <h4 class="page-title">List users by receive FFT token</h4>
                    </div>
                </div>
            </div>
            <div class="container-fluid">
                <row>
                    <div class="card-body bg-light">
                        <div class="form-group mb-0 text-left">
                            <h6 class="page-title">This list only show users have done 4 step and watch at least 30 min
                            </h6>

                        </div>
                        <div class="form-group mb-0 text-right">
                            <button id="btn_reload" class="btn btn-info waves-effect waves-light">Reload data</button>

                        </div>
                    </div>
                </row>
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table id="statistics_table" class="table table-striped table-bordered no-wrap">
                                        <thead>
                                            <tr>
                                                <th>TelegramID</th>
                                                <th>Full Name</th>
                                                <th>Email</th>
                                                <th>Total FFT</th>
                                                <th>Zoom time</th>
                                                <th>Ref TelegramID</th>
                                                <th>Wallet</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% users.forEach((user, index) => { %>
                                            <tr data-index="0">
                                                <td style=""><%= user.telegramID %></td>
                                                <td style=""><%= user.fullName %></td>
                                                <td style=""><%= user.email %> </td>
                                                <td class="ffttotal" style=""> <%= user.FTTTotal %> </td>
                                                <td style=""> <%= user.totalTime %> min </td>
                                                <td style=""> <%= user.refTelegramID %></td>
                                                <td style=""> <%= user.bep20 %></td>
                                            </tr>
                                            <% }) %>
                                        </tbody>
                                        <tfoot>
                                            <tr>

                                            </tr>
                                        </tfoot>
                                    </table>
                                    <ul class="pagination">
                                        <% if (page > 1) { %>
                                        <li class="page-item ">
                                            <a class="page-link" href="/statistics?page=<%= page - 1 %>"> Prev </a>
                                        </li>
                                        <% } %>
                                        <% range.map((i) => { %>
                                        <li class="page-item <%= page === i ? "active" : "" %>">
                                            <% if (i === "...") { %>
                                            <span> <%= i %> </span>
                                            <% } else { %>
                                            <a class="page-link" href="/statistics?page=<%= i %>"><%= i %></a>
                                            <% } %>
                                        </li>
                                        <% }) %>
                                        <% if (page < totalPages) { %>
                                        <li class="page-item ">
                                            <a class="page-link" href="/statistics?page=<%= page + 1 %>"> Next </a>
                                        </li>
                                        <% } %>

                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->

        </div>
        <!-- ============================================================== -->
        <!-- End Page wrapper  -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="/js/theme/jquery.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
